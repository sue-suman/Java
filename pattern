public class Main
{
    void fun(){
        int n=5;
        int i=1;
        while(i<=n){
            int j=1;
            while(j<=n){
                if(i==3){
                    //System.out.print(" ");
                    break;
                }
                j++;
                System.out.print("*"); 
            }
            System.out.println();
            i++;
        }
    }
	public static void main(String[] args) {
		Main obj=new Main();
		obj.fun();
	}
}
