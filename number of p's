public class Main
{	public static void main(String[] args) {
        String str= "peter picker picked a" + "pack of pickled peppers";
        int len = str.length();
        System.out.println(len);
        int count=0;
        
        for(int i=0;i<len;i++){
            if (str.charAt(i)!='p'){
                continue;
            }
            else{
                count++;
            }
        }
    }
}
